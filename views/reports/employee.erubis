<table border="1">
  <thead>
    <th>FECHA</th>
    <th>HORA DE ENTRADA</th>
    <th>HORA DE SALIDA</th>
    <th>TOTAL HORAS</th>
  </thead>
    <% for clock in object[:clocks] %>
    <tr>
      <td><%= clock.clock_in %></td>
      <td><%= clock.clock_in %></td>
      <td><%= clock.clock_out %></td>
      <td><%= clock.total_hours %></td>
    </tr>
    <% end %>
</table>
